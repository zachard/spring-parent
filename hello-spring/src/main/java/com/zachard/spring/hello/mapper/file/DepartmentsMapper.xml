<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zachard.spring.hello.mapper.DepartmentsMapper">
    
    <select id="queryByNo" resultType="Departments">
        SELECT dept_no deptNo, dept_name deptName FROM departments WHERE dept_no = #{deptNo} 
    </select>
    
</mapper>